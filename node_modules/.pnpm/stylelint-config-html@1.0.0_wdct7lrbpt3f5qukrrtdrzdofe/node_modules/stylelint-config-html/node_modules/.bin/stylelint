#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/Kevin/mon_site/vue3-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/Kevin/mon_site/vue3-template/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@14.8.2/node_modules/stylelint/bin/stylelint.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint@14.8.2/node_modules/stylelint/bin/stylelint.js" "$@"
fi
